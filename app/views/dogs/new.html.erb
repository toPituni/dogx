<h1>New Dog</h1>

<%= form_for(@dog) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :pick_up_address %>
  <%= f.text_field :pick_up_address %>

  <%= f.label :special_requirements %>
  <%= f.text_field :special_requirements %>
  <%= f.submit %>

  <%= link_to "Cancel", dogs_path %>
<% end %>
